.container {
    background-color: #F2f2f2;
}

.consulting_entrance_list{
  display: flex;
  align-content: space-between;
  flex-wrap: wrap;
  padding: 0;
}

.consulting_entrance_item{
  margin: auto auto;
}

.counter{
  display: flex;
  display: -webkit-flex;
}


.display_content_list{
  display: inline-flex;
  align-content: space-around;
  flex-wrap: wrap;
  padding: 0;
  margin: 0;
}


.display_content{
  display: inline-flex;
  width: 100%;
  align-content: space-around;
  padding: 0;
  margin: 0;
}


.display_content_pic{
  width: 150rpx;
  height: 150rpx;
  padding: 0;
  margin: 0;
}

.display_content_text{
  align-content: space-around;
}